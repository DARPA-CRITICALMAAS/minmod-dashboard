  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Dash App (Dev Mode)",
      "type": "debugpy",
      "request": "launch",
      "module": "poetry",
      "args": [
        "run",
        "python",
        "app.py",
        "dev"
      ],
    },
    {
      "name": "Python: Test",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "console": "integratedTerminal"
    }
  ]
}